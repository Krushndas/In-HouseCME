<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Add/Edit Educator Test Suite" verbose="1">
    <listeners>
        <listener class-name="com.CME.utils.TestListener" />
    </listeners>
    <parameter name="type" value="individual"/>
    <test name="Register Educator Test">
        <classes>
            <class name="com.CME.TestCases.RegisterEducatorTest"/>
        </classes>
    </test>
    <test name="Educator Login Test">
        <classes>
            <class name="com.CME.TestCases.EducatorLoginTest"/>
        </classes>
    </test>
    <test name="Educator Change Password Test">
        <classes>
            <class name="com.CME.TestCases.EducatorChangePasswordTest"/>
        </classes>
    </test>
    <test name="Admin Edit Educator Profile Test">
        <classes>
            <class name="com.CME.TestCases.AdminEditEducatorTest"/>
        </classes>
    </test>
</suite>