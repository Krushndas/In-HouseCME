<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Add/Edit Sponsor Test Suite" verbose="1">
    <listeners>
        <listener class-name="com.CME.utils.TestListener" />
    </listeners>
    <parameter name="type" value="individual"/>
    <test name="Admin Add Sponsor Test">
        <classes>
            <class name="com.CME.TestCases.AdminAddSponsorTest"/>
        </classes>
    </test>
        <test name="Sponsor Login Test">
            <classes>
                <class name="com.CME.TestCases.SponsorLoginTest"/>
            </classes>
        </test>
        <test name="Sponsor Change Password Test">
            <classes>
                <class name="com.CME.TestCases.SponsorChangePasswordTest"/>
            </classes>
        </test>
        <test name="Sponsor Edit Profile Test">
            <classes>
                <class name="com.CME.TestCases.SponsorEditProfileTest"/>
            </classes>
        </test>
</suite>